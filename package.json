{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:contentlayer": "contentlayer dev",
    "dev:nextjs": "next dev",
    "start": "next start",
    "build": "run-p build:*",
