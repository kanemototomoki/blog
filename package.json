    "build:nextjs": "npm run type-check && next build && next export -o dist",
    "lint": "next lint --dir src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "type-check": "tsc --noEmit",
    "gen-mdx": "./blog/draft/bin/gen-mdx"
  },
  "dependencies": {
    "@code-hike/mdx": "^0.7.4",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.9.2",
    "@mantine/hooks": "^5.9.2",
    "@mantine/next": "^5.9.2",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@next/mdx": "^13.0.6",
    "@tabler/icons": "^1.116.1",
    "@vercel/analytics": "^0.1.6",
    "contentlayer": "^0.2.9",
    "github-slugger": "^2.0.0",
    "next": "13.0.6",
    "next-contentlayer": "^0.2.9",
    "next-mdx-remote": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scroll": "^1.8.9",
