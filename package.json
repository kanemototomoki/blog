    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.1.0",
    "remark-external-links": "^9.0.1",
    "remark-gfm": "^3.0.1",
    "remark-toc": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "18.11.12",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/react-scroll": "^1.8.5",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "typescript": "4.9.4"
  }
